{
  "name": "RSS Feed to Telegram Alert",
  "nodes": [
    {
      "parameters": {
        "url": "https://example.com/feed",
        "pollTimes": {
          "item": [
            {
              "interval": 30,
              "unit": "minutes"
            }
          ]
        }
      },
      "id": "1",
      "name": "RSS Feed Read",
      "type": "n8n-nodes-base.rssFeedRead",
      "typeVersion": 1,
      "position": [240, 300]
    },
    {
      "parameters": {
        "chatId": "YOUR_CHAT_ID",
        "text": "New post: {{ $json.title }}\n{{ $json.link }}"
      },
      "id": "2",
      "name": "Send to Telegram",
      "type": "n8n-nodes-base.telegram",
      "typeVersion": 1,
      "position": [460, 300],
      "credentials": {
        "telegramApi": {
          "id": "YOUR_TELEGRAM_CREDENTIAL_ID",
          "name": "Telegram account"
        }
      }
    }
  ],
  "connections": {
    "RSS Feed Read": {
      "main": [
        [
          {
            "node": "Send to Telegram",
            "type": "main",
            "index": 0
          }
        ]
      ]
    }
  },
  "active": false,
  "settings": {},
  "versionId": "1"
}
